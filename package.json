{
  "name": "@weavy/uikit-react",
  "version": "21.0.0",
  "author": "Weavy",
  "description": "React components UI-kit for Weavy",
  "homepage": "https://github.com/weavy/weavy-uikit-react",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "lib",
    "*.json",
    "*.md"
  ],
  "main": "./dist/weavy.es5.umd.cjs",
  "module": "./dist/weavy.es5.esm.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/weavy.esm.js",
      "require": "./dist/weavy.umd.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "start": "run-p auth dev",
    "dev": "vite --open",
    "auth": "weavy auth-server",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@lit/react": "^1.0.2",
    "@weavy/uikit-web": "^21.0.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.3.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.2"
  },
  "engines": {
    "node": ">=19.3.0",
    "npm": ">=9.2.0"
  }
}
