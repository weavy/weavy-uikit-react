{
  "name": "@weavy/uikit-react",
  "version": "19.1.2",
  "author": "Weavy",
  "description": "React UI-kit for Weavy",
  "homepage": "https://github.com/weavy/weavy-uikit-react",
  "license": "MIT",
  "main": "dist/cjs/index.es5.js",
  "module": "dist/esm/index.es5.js",
  "types": "dist/index.d.ts",
  "style": "dist/css/weavy.css",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/css/weavy.css"
    },
    "./dist/css/": {
      "default": "./dist/css/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "^16.14.55",
    "@types/react-dom": "^16.9.24",
    "@types/react-modal": "^3.16.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.69.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">= 16",
    "react-dom": ">= 16"
  },
  "scripts": {
    "clean": "rimraf --glob dist/*",
    "build": "npm-run-all clean --parallel css js",
    "css": "sass src/scss:dist/css --no-source-map",
    "js": "rollup -c",
    "watch": "npm-run-all clean --parallel watch-css watch-js",
    "watch-css": "sass src/scss:dist/css --watch",
    "watch-js": "rollup -c --watch"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.11.1",
    "@codemirror/lang-markdown": "^6.2.3",
    "@mdi/js": "^7.3.67",
    "@mdi/react": "^1.6.0",
    "@microsoft/signalr": "^7.0.14",
    "@popperjs/core": "^2.11.8",
    "@uiw/react-codemirror": "^4.21.21",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.10",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "pdfjs-dist": "3.11.174",
    "react-dropzone": "^14.2.3",
    "react-modal": "^3.14.4",
    "react-popper": "^2.3.0",
    "react-query": "^3.39.3"
  },
  "engines": {
    "node": ">=19.3.0",
    "npm": ">=9.2.0"
  }
}
